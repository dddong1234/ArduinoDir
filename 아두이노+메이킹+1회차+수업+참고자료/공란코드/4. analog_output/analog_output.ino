void setup(){
pinMode(?,?); //LED와 연결한 핀(9)을 출력으로 설정한다.
}
void loop(){
analogWrite(9,?); // LED연결한 핀을 0의 값으로 출력한다.
delay(300); // 0.3초 기다린다.
analogWrite(9,?); // LED연결한 핀을 50의 값으로 출력한다.
delay(300); // 0.3초 기다린다.
analogWrite(9,?); // LED연결한 핀을 100의 값으로 출력한다.
delay(300); // 0.3초 기다린다.
analogWrite(9,?); // LED연결한 핀을 150의 값으로 출력한다.
delay(300); // 0.3초 기다린다.
analogWrite(9,?); // LED연결한 핀을 200의 값으로 출력한다.
delay(300); // 0.3초 기다린다.
}
